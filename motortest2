#define ENB 5
#define IN3 7
#define IN4 6

void motor2() {
  digitalWrite(IN3, HIGH);
  digitalWrite(IN4, LOW);
  analogWrite(ENB, 100); // Motor 2'yi düşük hızda hareket ettir
}

void setup() {
  pinMode(ENB, OUTPUT);
  pinMode(IN3, OUTPUT);
  pinMode(IN4, OUTPUT);
}

void loop() {
  motor2(); // Motor 2'yi kontrol et
}
